	<bean id="s3" name="s3" class="spring_core_y.basic.Student">
		<property name="name">
			<value>jane</value>
		</property>

		<property name="age">
			<value>24</value>
		</property>
	</bean>


	<!-- more bean definitions go here -->
</beans>

